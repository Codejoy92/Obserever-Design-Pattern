		Node createNode = new Node(valuesIn);
		if(null == root) {
			root = createNode;
			return;
		}
		
		Node currentNode = root;
		Node parentNode = null;
		
		while(true){
			parentNode =currentNode;
			
			if(valuesIn.keySet() < currentNode.getData().keySet()){				
				current = current.left;
				if(current==null){
					parent.left = newNode;
					return;
				}
			}else{
				current = current.right;
				if(current==null){
					parent.right = newNode;
					return;
				}
			}
	}
}
